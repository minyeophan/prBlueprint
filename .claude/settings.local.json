{
  "permissions": {
    "allow": [
      "Bash(dir:*)",
      "Bash(gh repo view:*)",
      "WebFetch(domain:github.com)",
      "WebSearch",
      "Bash(./gradlew build:*)",
      "Bash(./gradlew.bat build:*)",
      "Bash(tree:*)",
      "Bash(git add:*)",
      "Bash(git commit:*)",
      "Bash(git push:*)",
      "Bash(git filter-branch:*)",
      "Bash(git stash:*)",
      "Bash(FILTER_BRANCH_SQUELCH_WARNING=1 git filter-branch:*)",
      "Bash(git checkout:*)",
      "Bash(git reset:*)",
      "Bash(git update-ref:*)",
      "Bash(docker-compose up:*)",
      "Bash(.gradlew.bat bootRun:*)",
      "Bash(findstr:*)",
      "Bash(ForEach-Object { git update-ref -d $_ })",
      "Bash(while read hash subject)",
      "Bash(do echo \"=== $hash ===\")",
      "Bash(done)",
      "Bash(do git show -s --format=%B $hash)",
      "Bash(echo:*)",
      "Bash(git fetch:*)",
      "Bash(curl.exe:*)",
      "Bash(xargs -I {} git show:*)",
      "Bash(git log:*)",
      "Bash(git rm:*)",
      "Bash(git cherry-pick:*)",
      "Bash(git merge:*)"
    ]
  }
}
